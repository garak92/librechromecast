{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.4",
    "@mui/material": "^5.2.4",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "dotenv": "^10.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "http-server": "^14.0.0",
    "local-ipv4-address": "^0.0.2",
    "prettier": "^2.5.1",
    "react": "^16.14.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-player-controls": "^1.1.0",
    "react-redux": "^7.2.6",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron . \"",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "server": "node ../app.js",
    "client": "cross-env BROWSER=none npm start",
    "electron": "wait-on http://localhost:3000 && electron .",
    "dev": "concurrently \"npm run server\" \"npm run client\"\" npm run electron\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "electron-redux-devtools": "^0.3.4",
    "wait-on": "^6.0.0"
  }
}
